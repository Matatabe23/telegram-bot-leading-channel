{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "qugor-arts",
	"version": "0.1.0",
	"identifier": "com.qugor.telegrambot",
	"build": {
		"frontendDist": "../dist",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build"
	},
	"app": {
		"windows": [
			{
				"title": "QugorArts",
				"width": 1400,
				"height": 700,
				"minWidth": 300,
				"minHeight": 400,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
